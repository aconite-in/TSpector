{
    "cucumberautocomplete.steps": [
        "Steps/*.ts"
    ],
    "cucumberautocomplete.syncfeatures": "features/*.feature",
    "cucumberautocomplete.strictGherkinCompletion": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        ".gitignore": true,
        "**/package-lock.json": true,
        "**/package.json": true,
        "**/*.js": true,
        "**/*.js.map": true,
        "**/node_modules": true
    }
}