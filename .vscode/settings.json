{
    "cucumberautocomplete.steps": [
        "Steps/*.ts"
    ],
    "cucumberautocomplete.syncfeatures": "features/*.feature",
    "cucumberautocomplete.strictGherkinCompletion": true,
    "cucumberautocomplete.pages": {
        "users": "pages/*.ts",
    },
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "files.exclude": {
        "**/.git": true,
        ".gitignore": true,
        "**/package-lock.json": true,
        "**/package.json": true,
        "**/*.js": true,
        "**/*.js.map": true,
        "**/node_modules": true,
        "tslint.json": true
    }
}